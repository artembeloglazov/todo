{% extends 'base.html.twig' %}

{% block title %}Calendar{% endblock %}

{% block body %}
    <h1>Calendar</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ calendar.id }}</td>
            </tr>
            <tr>
                <th>IsExport</th>
                <td>{{ calendar.isExport ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>FileName</th>
                <td>{{ calendar.fileName }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ calendar.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_calendar_index') }}">back to list</a>

    <a href="{{ path('app_calendar_edit', {'id': calendar.id}) }}">edit</a>

    {{ include('calendar/_delete_form.html.twig') }}
{% endblock %}
